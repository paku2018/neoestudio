    $(document).on('click', '.quantityInc', function () {
        var product_id = $(this).attr('data-pid');
        var price = $(this).attr('data-price');
        var total_quantity = parseInt($(this).parents('form').find('input[name="total_quantity"]').val());
        var total_price = parseFloat($(this).parents('form').find('input[name="total_price"]').val());
        var shipping = $(this).parents('form').find('input[name="shipping"]').val();
        var product_quantity = parseInt($('#quantity-' + product_id).html());
        var productInc = (product_quantity + 1);
        var totalQuantity = (total_quantity + 1);
        console.log(price);
        var totalPrice = parseFloat(price) + parseFloat(shipping);
        if (total_price > 0) {
            totalPrice = (total_price + parseFloat(price));
        }
        $('#quantity-' + product_id).html(productInc);
        $(this).parents('form').find('input[name="total_quantity"]').val(totalQuantity);
        $(this).parents('form').find('input[name="total_price"]').val((totalPrice).toFixed(2));
        $('.total-product').html((totalPrice).toFixed(2) + '&euro;');
        $('#pid-'+product_id).val(productInc);
    });

    $(document).on('click', '.quantityDec', function () {
        var product_id = $(this).attr('data-pid');
        var price = $(this).attr('data-price');
        var total_quantity = parseInt($(this).parents('form').find('input[name="total_quantity"]').val());
        var total_price = parseFloat($(this).parents('form').find('input[name="total_price"]').val());
        var product_quantity = parseInt($('#quantity-' + product_id).html());
        var productInc = product_quantity - 1;
        var totalQuantity = total_quantity - 1;
        var totalPrice = 0;
        if (total_price > 0) {
            totalPrice = (total_price - parseFloat(price));
        }
        if (totalQuantity == 0) {
            $('.total-product').html('0&euro;');
            $(this).parents('form').find('input[name="total_price"]').val(0);
        }
        if (productInc < 0) {
            return false;
        }
        $('#quantity-' + product_id).html(productInc);
        $(this).parents('form').find('input[name="total_quantity"]').val(totalQuantity);
        if (totalQuantity > 0) {
            $(this).parents('form').find('input[name="total_price"]').val((totalPrice).toFixed(2));
            $('.total-product').html(parseFloat((totalPrice).toFixed(2)) + '&euro;');
        }
    });
