            success: function (result) {
                var data = JSON.parse(result);
                $.each(data, function (k, v) {
                    if(k == 2){
                        $('#prices-' + k).html(v);
                    }
                    else{
                        $('#prices-' + k).html(v+' ‚Ç¨');
                    }
                });
                var totalP = data[imageIndex];
                if(imageIndex == 0){
                    totalP = data[1];
                }
                if(imageIndex == 1){
                    totalP = (data[0] * 2);
                }
                if(imageIndex == 2){
                    totalP = data[0];
                }
                $('.total-package').html(totalP + ' &euro; (' + meses[imageIndex].toLocaleLowerCase() + ')');
